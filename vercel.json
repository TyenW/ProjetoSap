{
	"version": 2,
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"builds": [
		{ "src": "api/**/*.mjs", "use": "@vercel/node" },
		{ "src": "dist/**", "use": "@vercel/static" }
	],
	"routes": [
		{ "src": "/api/(.*)", "dest": "/api/$1" },
		{ "src": "/(.*)", "dest": "/dist/$1" }
	]
}
